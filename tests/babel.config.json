{
  "sourceMaps": "inline",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        },
        "modules": "commonjs",
        "exclude": [
          "@babel/plugin-transform-regenerator"
        ],
        "useBuiltIns": false
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "classic",
        "throwIfNamespace": true,
        "pure": true,
        "useBuiltIns": true
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "allowNamespaces": false,
        "allowDeclareFields": true,
        "optimizeConstEnums": true
      }
    ]
  ]
}
